{
  "manifest_version": 2,
  "name": "Github Image Squisher",
  "description": "Squish screenshots in your pull request",
  "version": "1.0",
  "browser_action": {
    "default_icon": "assets/squasher.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
